{
  "all": {
    "watchAdditionalPaths": ["app/frontend/libs"],
    "additionalEntrypoints": [
      "app/frontend/libs/*.ts",
      "app/frontend/assets/icons/*",
      "app/frontend/assets/images/*"
    ]
  },
  "production": {
    "publicOutputDir": "dist"
  },
  "development": {
    "autoBuild": true,
    "publicOutputDir": "dist-dev",
    "port": 3036
  },
  "test": {
    "autoBuild": true,
    "publicOutputDir": "dist-test",
    "port": 3037
  }
}
