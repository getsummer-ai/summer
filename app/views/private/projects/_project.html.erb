<div class="border-2 p-2 border-accent" id="<%= dom_id project %>">
  <h2 class="text-2xl font-extrabold"><%= project.name.upcase %></h2>
  <p><span class="badge badge-accent"> <%= project.domain %> </span></p>

  <% if rand(2) == 1 %>
    <p class="underline text-xs mt-4">Last button calling: 20 minutes ago</p>
  <% end %>

  <p><span class="badge mt-4 badge-ghost">Plan: <%= project.plan %> </span></p>
  <p><span class="badge badge-ghost">Created: <%= project.created_at %> </span></p>

  <p><span class="badge mt-4">Articles amount: <%= rand(10) %> </span></p>
  <p><span class="badge">Button showed: <%= rand(1000) %> </span></p>
  <p><span class="badge">Button clicks: <%= rand(100) %> </span></p>


</div>
